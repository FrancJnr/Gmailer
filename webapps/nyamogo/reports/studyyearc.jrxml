	<queryString>
		<![CDATA[SELECT degree_name, student_id, student_name, get_cumm_credit(student_degree_id, max_session_id) as credits,
(CASE WHEN get_cumm_credit(student_degree_id, max_session_id) < 52 THEN '1. First Year'
WHEN get_cumm_credit(student_degree_id, max_session_id) < 103 THEN '2. Second Year'
WHEN get_cumm_credit(student_degree_id, max_session_id) < 154 THEN '3. Third Year' ELSE '4. Forth Year' END) as student_year
FROM
(SELECT degree_name, student_degree_id, student_id, student_name, max(session_id) as max_session_id
FROM vw_student_session_summary
WHERE (session_year = '$P!{filterid}')
GROUP BY degree_name, student_degree_id, student_id, student_name) as a
ORDER BY degree_name, get_cumm_credit(student_degree_id, max_session_id);]]>
	</queryString>
