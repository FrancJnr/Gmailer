	<queryString>
		<![CDATA[SELECT b.degree_name, b.student_year, count(b.student_degree_id) as students
FROM
(SELECT degree_name, student_degree_id,
(CASE WHEN get_cumm_credit(student_degree_id, max_session_id) < 52 THEN '1. First Year'
WHEN get_cumm_credit(student_degree_id, max_session_id) < 103 THEN '2. Second Year'
WHEN get_cumm_credit(student_degree_id, max_session_id) < 154 THEN '3. Third Year' ELSE '4. Forth Year' END) as student_year
FROM
(SELECT degree_name, student_degree_id, max(session_id) as max_session_id
FROM vw_student_session_summary
WHERE (session_year = '$P!{filterid}')
GROUP BY degree_name, student_degree_id) as a) as b
GROUP BY b.degree_name, b.student_year
ORDER BY b.degree_name, b.student_year;]]>
	</queryString>
