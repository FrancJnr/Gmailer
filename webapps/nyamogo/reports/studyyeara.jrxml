	<queryString>
		<![CDATA[SELECT degree_name, student_id, student_name, get_cumm_credit(student_degree_id, session_id) as credits,
(CASE WHEN get_cumm_credit(student_degree_id, session_id) < 52 THEN '1. First Year'
WHEN get_cumm_credit(student_degree_id, session_id) < 103 THEN '2. Second Year'
WHEN get_cumm_credit(student_degree_id, session_id) < 154 THEN '3. Third Year' ELSE '4. Forth Year' END) as student_year
FROM vw_student_session_summary
WHERE (session_id = '$P!{filterid}')
ORDER BY degree_name, get_cumm_credit(student_degree_id, session_id);]]>
	</queryString>
