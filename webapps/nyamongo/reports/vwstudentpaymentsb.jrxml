	<queryString>
		<![CDATA[SELECT vwstudentpayments.accountnumber, vwstudentpayments.studentid, vwstudentpayments.studentname, 
vwstudentpayments.studentpaymentid, vwstudentpayments.amount, vwstudentpayments.quarterid,
vwbanks.bankname, cast(vwstudentpayments.approvedtime as date) as approveddate, 
cast(vwstudentpayments.approvedtime as time) as approvedtime
FROM vwstudentpayments INNER JOIN vwbanks ON vwstudentpayments.terminalid = vwbanks.terminalid
WHERE (vwstudentpayments.approved = true) AND (vwstudentpayments.quarterid = '$P!{filterid}')
AND (vwstudentpayments.terminalid = '$P!{terminalid}')
ORDER BY cast(vwstudentpayments.approvedtime as date), cast(vwstudentpayments.approvedtime as time);]]>
